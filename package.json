{
  "name": "Shopsifu",
  "version": "1.0.0",
  "description": "Shopsifu",
  "author": "TedyDev",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main",
    "dev": "nest start --watch",
    "dev:docker": "git fetch origin develop && git reset --hard origin/develop && docker compose up -d --build",
    "debug": "nest start --debug --watch",
    "update:package": "npx npm-check-updates -u && npm install",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "studio": "prisma studio",
    "seed:email": "nestjs-command seed:emails",
    "init-seed-data": "ts-node -r tsconfig-paths/register initialScript",
    "create-permissions": "ts-node -r tsconfig-paths/register initialScript/create-permissions.ts",
    "rollback:email": "nestjs-command rollback:emails",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest --config test/jest.json --runInBand --passWithNoTests --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config test/jest.json --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.775.0",
    "@aws-sdk/lib-storage": "^3.775.0",
    "@aws-sdk/s3-request-presigner": "^3.775.0",
    "@keyv/redis": "^4.4.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.0",
    "@prisma/client": "^6.3.1",
    "@react-email/components": "^0.3.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.51.0",
    "cache-manager": "^6.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "googleapis": "^146.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.1",
    "ms": "^2.1.3",
    "nestjs-i18n": "^10.5.1",
    "nestjs-pino": "^4.4.0",
    "nestjs-zod": "^4.3.1",
    "otpauth": "^9.3.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "redis": "^5.0.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.1.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.2",
    "pino-pretty": "^11.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.17",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.3.1",
    "prisma-json-types-generator": "^3.2.3",
    "react-email": "^4.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "lint-staged": {
    "{src,test}/**/*.{ts,js,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
